{
  "name": "vscode-focus-theme",
  "displayName": "Focus Theme",
  "description": "An opinionated clean and minimalistic VS Code theme.",
  "license": "SEE LICENSE IN LICENSE",
  "version": "1.0.0",
  "publisher": "ekorzun",
  "preview": true,
  "engines": {
    "vscode": "^1.22.0"
  },
  "icon": "icon.png",
  "keywords": [
    "theme",
    "vscode",
    "vscode-theme",
    "minimal",
    "dark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ekorzun/vscode-focus-theme.git"
  },
  "bugs": {
    "url": "https://github.com/ekorzun/vscode-focus-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Focus Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/focus-dark-color-theme.json"
      }
    ]
  },
  "scripts": {
    "dev": "next",
    "watch": "chokidar \"src_themes/**/*.js\" -c \"npm run build_theme\"",
    "build_site": "next build",
    "build_theme": "node ./src_themes/build.js",
    "build": "npm run build_site && npm run build_theme"
  },
  "dependencies": {
    "@zeit/next-stylus": "^1.0.1",
    "classnames": "^2.2.6",
    "next": "^9.2.1",
    "next-images": "^1.3.0",
    "pure-react-carousel": "^1.24.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-responsive-carousel": "^3.1.51",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1",
    "stylus": "^0.54.7"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0"
  }
}
